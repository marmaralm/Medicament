<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nestedScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstFragment">

    <Button
        android:id="@+id/button_medicamentos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/medicamentos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.19"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp" />

    <Button
        android:id="@+id/button_grupos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/grupos_farmacoterapeuticos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button_medicamentos"
        app:layout_constraintStart_toStartOf="@+id/button_medicamentos"
        app:layout_constraintTop_toBottomOf="@+id/button_medicamentos"
        app:layout_constraintVertical_bias="0.19999999" />

    <Button
        android:id="@+id/button_pesquisa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/pesquisa"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button_grupos"
        app:layout_constraintStart_toStartOf="@+id/button_grupos"
        app:layout_constraintTop_toBottomOf="@+id/button_grupos"
        app:layout_constraintVertical_bias="0.19" />

    <Button
        android:id="@+id/button_sair"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sair"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button_pesquisa"
        app:layout_constraintStart_toStartOf="@+id/button_pesquisa"
        app:layout_constraintTop_toBottomOf="@+id/button_pesquisa"
        app:layout_constraintVertical_bias="0.19" />

</androidx.constraintlayout.widget.ConstraintLayout>